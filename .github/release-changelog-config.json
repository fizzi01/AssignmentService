{
  "template": "#${{CHANGELOG}}\n\n${{RELEASE_DIFF}}\n",
  "categories": [
    { "title": "� Enhancements", "labels": ["enhancement"] },
    { "title": "� Bug Fixes", "labels": ["bug"] },
